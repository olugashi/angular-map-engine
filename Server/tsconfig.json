{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "lib": [
          "es6",
          "dom"
      ],
      "noImplicitAny": false,
      "sourceMap": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "moduleResolution": "node",
      "noUnusedLocals": false,
      "strict": true,  
      "outDir": "./out"
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ]
}